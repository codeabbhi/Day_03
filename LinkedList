import java.util.*;
class Main {
    public static void main(String[] args) {
        LinkedList<Integer>ll = new LinkedList<>();
        ll.add(1);
        ll.add(2);
        ll.add(4);
        ll.set(2,3);
        
        Object [] arr = ll.toArray();
        for(int i=0;i<ll.size();i++){
             System.out.print(ll.get(i) +" ");
        }
        
       
    }
}
